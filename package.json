{
  "name": "xiv_tracker",
  "version": "0.5.4",
  "description": "Tracking character information for Final Fantasy XIV",
  "main": "src/server/server.js",
  "scripts": {
    "start": "babel-node src/server/server.js",
    "build": "webpack -p",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/server.js --exec babel-node",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "mocha",
    "test:watch": "mocha --watch",
    "heroku-postbuild": "webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ksnovak/xiv_tracker"
  },
  "repo_root": "https://github.com/ksnovak/xiv_tracker",
  "homepage": "https://github.com/ksnovak/xiv_tracker#readme",
  "author": "Kevin Novak",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.21.2",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "lodestone-parser": "^1.1.1",
    "prop-types": "^15.7.2",
    "query-string": "^6.4.0",
    "react": "^16.8.5",
    "react-animate-on-change": "^2.1.1",
    "react-dom": "^16.8.5",
    "react-image": "^2.1.1",
    "react-select": "^2.4.2",
    "react-spinkit": "^3.0.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.4.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.1",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^2.0.1",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "prettier-eslint": "^8.8.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  }
}
